# Tsükkli skript

# Välimine tsükkel ridade jaoks
for (( i = 1; i <= 5; i++ ))      ### Välimine tsükkel - iga rida
do
    # Väljasta rea number
    echo -n "$i.  "                ### Väljasta ridade number

    # Sisemine tsükkel tähtede (*) jaoks igas reas
    for (( j = 1; j <= 5; j++ ))   ### Sisemine tsükkel - iga täht
    do
        echo -n "* "               ### Väljasta täht *
    done

    # Lõpeta rida ja mine järgmisele
    echo ""                       ### Täiendame rea lõpus
done
